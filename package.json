    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.5.1",
    "prop-types": "^15.6.2",
